{
  "name": "mini-dev",
  "version": "0.1.0",
  "description": "minimal dev with coffeescript",
  "main": "src/index.coffee",
  "scripts": {
    "dev": "npm-run-all --parallel client server",
    "client": "watchify -t coffeeify src/index.coffee -o dist/bundle.js --debug --verbose --extension='.coffee'",
    "server": "live-server --cors --no-browser --ignore=node_modules",
    "deploy": "surge -p server/public -d https://YOUR-DOMAIN.surge.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "akralj",
  "license": "MIT",
  "devDependencies": {
    "coffeeify": "^3.0.1",
    "coffeescript": "^2.0.2",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.1",
    "surge": "^0.19.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "axios": "^0.16.2"
  }
}
